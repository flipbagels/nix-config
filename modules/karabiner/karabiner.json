{
  "title": "Swap Double Quote and At Symbol (UK Keyboard)",
  "rules": [
    {
      "description": "Swap @ and double quote",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "2",
            "modifiers": ["left_shift"]
          },
          "to": [
            {
              "key_code": "quote",
              "modifiers": ["left_shift"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "quote",
            "modifiers": ["left_shift"]
          },
          "to": [
            {
              "key_code": "2",
              "modifiers": ["left_shift"]
            }
          ]
        }
      ]
    }
  ]
}
